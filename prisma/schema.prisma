generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                 Int      @id @default(autoincrement())
  username           String
  hashedPassword     String
  hashedRefreshToken String?
  createdAt          DateTime @default(now())
  email              String
}
